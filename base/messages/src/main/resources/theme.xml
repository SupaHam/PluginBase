<?xml version="1.0" encoding="ISO-8859-1"?>
<theme>
    <SUCCESS>
        <tag>+</tag>
        <color>GREEN</color>
    </SUCCESS>
    <ERROR>
        <tag>-</tag>
        <color>RED</color>
    </ERROR>
    <PLAIN>
        <tag>.</tag>
        <color>RESET</color>
    </PLAIN>
    <IMPORTANT>
        <tag>!</tag>
        <style>BOLD</style>
    </IMPORTANT>
    <IMPORTANT2>
        <tag>*</tag>
        <style>ITALIC</style>
    </IMPORTANT2>
    <IMPORTANT3>
        <tag>_</tag>
        <style>UNDERLINE</style>
    </IMPORTANT3>
    <HELP>
        <tag>h</tag>
        <color>WHITE</color>
    </HELP>
    <INFO>
        <tag>i</tag>
        <color>AQUA</color>
    </INFO>
    <SORRY>
        <tag>$</tag>
        <color>DARK_GRAY</color>
    </SORRY>
    <RETRY>
        <tag>,</tag>
        <color>GRAY</color>
    </RETRY>
    <DO_THIS>
        <tag>~</tag>
        <color>BLUE</color>
    </DO_THIS>
    <VALUE>
        <tag>v</tag>
        <color>DARK_GREEN</color>
    </VALUE>
    <TITLE>
        <tag>t</tag>
        <color>DARK_AQUA</color>
    </TITLE>
    <CMD_USAGE>
        <tag>c</tag>
        <color>WHITE</color>
    </CMD_USAGE>
    <CMD_FLAG>
        <tag>f</tag>
        <color>GOLD</color>
        <style>ITALIC</style>
    </CMD_FLAG>
    <OPT_ARG>
        <tag>o</tag>
        <color>GOLD</color>
    </OPT_ARG>
    <REQ_ARG>
        <tag>r</tag>
        <color>GREEN</color>
    </REQ_ARG>
    <CMD_HIGHLIGHT>
        <tag>C</tag>
        <style>BOLD</style>
    </CMD_HIGHLIGHT>
</theme>
