<?xml version="1.0" encoding="ISO-8859-1"?>
<theme>
    <SUCCESS>
        <tag>+</tag>
        <color>GREEN</color>
    </SUCCESS>
    <ERROR>
        <tag>-</tag>
        <color>RED</color>
    </ERROR>
    <PLAIN>
        <tag>.</tag>
        <color>RESET</color>
    </PLAIN>
    <IMPORTANT>
        <tag>!</tag>
        <style>BOLD</style>
    </IMPORTANT>
    <HELP>
        <tag>h</tag>
        <color>WHITE</color>
    </HELP>
    <INFO>
        <tag>i</tag>
        <color>YELLOW</color>
    </INFO>
    <VALUE>
        <tag>v</tag>
        <color>DARK_GREEN</color>
    </VALUE>
</theme>
